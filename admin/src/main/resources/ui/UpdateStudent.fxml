<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane animated="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" text="Student Update Portal" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21"
            fx:controller="edu.duke.ece651.team2.admin.UserRegistrationController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Label layoutX="50.0" layoutY="50.0" text="Please Type Student ID Number: " />
                <TextField fx:id = "studentIDNumber" layoutX="50.0" layoutY="75.0" />
                <Label layoutX="250.0" layoutY="50.0" text="Please Type New Student Password: " />
                <TextField fx:id = "studentPassword" layoutX="350.0" layoutY="75.0" />
                <Label layoutX="450.0" layoutY="50.0" text="Please Type New Student Display Name: " />
                <TextField fx:id = "studentDisplayName" layoutX="350.0" layoutY="75.0" />
                <Button layoutX="150.0" layoutY="125.0" mnemonicParsing="false" text="Submit" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                        onAction="#onUpdateStudentSubmit"/>
                <Button layoutX="150.0" layoutY="250.0" mnemonicParsing="false" text="Go Back" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                        onAction="#handleStudentRegistration"/>
                <Button layoutX="150.0" layoutY="300.0" mnemonicParsing="false" text="Main Menu" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                        onAction="#returnToMainRegistration"/>
            </children></AnchorPane>
    </content>
</TitledPane>
